{
    "name": "twint-ch/twint-sdk",
    "description": "TWINT software development kit for PHP",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "Twint\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twint\\Sdk\\Tests\\Integration\\": "tests/integration",
            "Twint\\Sdk\\Tests\\Unit\\": "tests/unit",
            "Twint\\Sdk\\Tools\\": "tools/"
        }
    },
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lars.strojny@nfq.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-xsl": "*",
        "ext-dom": "*",
        "ext-openssl": "*",
        "phpro/soap-client": "^3.1.1",
        "php-soap/engine": "^2.4.0",
        "php-soap/psr18-transport": "^1.5.0",
        "guzzlehttp/guzzle": "^7.4",
        "webimpress/safe-writer": "^2.0",
        "azjezz/psl": "^2.5.0",
        "psr/clock": "^1.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "phpunit/phpunit": "^10.5",
        "laminas/laminas-code": "^4.8",
        "symfony/dotenv": "^6.4",
        "phpstan/phpstan": "^1.10.56",
        "symplify/easy-coding-standard": "^12.1",
        "slevomat/coding-standard": "^8.14",
        "phpstan/phpstan-strict-rules": "^1.5",
        "php-vcr/php-vcr": "dev-fix-stream-handler-set-option as 1.7.0",
        "php-standard-library/phpstan-extension": "^1.0",
        "symfony/event-dispatcher": "^6.4",
        "symfony/clock": "^6.4",
        "wiremock-php/wiremock-php": "dev-cloud-support as 2.39.0",
        "php-soap/wsdl-reader": "^0.10.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "cweagans/composer-patches": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lstrojny/php-vcr.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/lstrojny/wiremock-php.git"
        }
    ]
}
