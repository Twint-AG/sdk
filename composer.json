{
    "name": "twint-ch/twint-sdk",
    "description": "TWINT software development kit for PHP",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Twint\\Sdk\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lars.strojny@nfq.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-xsl": "*",
        "phpro/soap-client": "^1.7",
        "php-soap/engine": "^2.6",
        "php-soap/psr18-transport": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "laminas/laminas-code": "^4.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
