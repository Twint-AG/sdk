{
    "name": "twint-ch/twint-sdk",
    "description": "TWINT software development kit for PHP",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "Twint\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twint\\Sdk\\Tests\\": "tests/",
            "Twint\\Sdk\\Checks\\": "checks/"
        }
    },
    "authors": [
        {
            "name": "Lars Strojny",
            "email": "lars.strojny@nfq.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-xsl": "*",
        "ext-dom": "*",
        "ext-openssl": "*",
        "phpro/soap-client": "3.1.0",
        "php-soap/engine": "^2.4.0",
        "php-soap/psr18-transport": "^1.0",
        "guzzlehttp/guzzle": "^7.3",
        "beberlei/assert": "^3.2",
        "webimpress/safe-writer": "^2.0",
        "goetas-webservices/xsd-reader": "0.4.1"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "phpunit/phpunit": "^10.5",
        "laminas/laminas-code": "^4.8",
        "symfony/dotenv": "^6.4",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-beberlei-assert": "^1.1",
        "symplify/easy-coding-standard": "^12.1",
        "slevomat/coding-standard": "^8.14",
        "phpstan/phpstan-strict-rules": "^1.5",
        "php-vcr/php-vcr": "^1.7"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
