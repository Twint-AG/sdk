{
    "name": "twint-ag/sdk",
    "description": "PHP SDK for TWINT",
    "license": "MIT",
    "type": "library",
    "homepage": "https://twint.ch",
    "authors": [
        {
            "name": "TWINT",
            "email": "plugin@twint.ch",
            "homepage": "https://twint.ch",
            "role": "Maintainer"
        }
    ],
    "keywords": [
        "twint",
        "twint.ch",
        "switzerland",
        "checkout",
        "express checkout",
        "payment",
        "sdk",
        "api"
    ],
    "autoload": {
        "files": [
            "src/polyfill.php"
        ],
        "psr-4": {
            "Twint\\Sdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Twint\\Sdk\\Tests\\Integration\\": "tests/integration",
            "Twint\\Sdk\\Tests\\Unit\\": "tests/unit",
            "Twint\\Sdk\\Tools\\": "tools/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-xsl": "*",
        "ext-dom": "*",
        "ext-openssl": "*",
        "phpro/soap-client": "^3.3.0",
        "php-soap/engine": "^2.4.0",
        "php-soap/psr18-transport": "^1.5.0",
        "guzzlehttp/guzzle": "^7.4",
        "webimpress/safe-writer": "^2.0",
        "azjezz/psl": "^2.5.0",
        "psr/clock": "^1.0",
        "veewee/xml": "^3.0.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "phpunit/phpunit": "^10.5.29",
        "laminas/laminas-code": "^4.14.0",
        "symfony/dotenv": "^6.4.10",
        "phpstan/phpstan": "^1.11.10",
        "symplify/easy-coding-standard": "^12.3.5",
        "slevomat/coding-standard": "^8.15.0",
        "phpstan/phpstan-strict-rules": "^1.6.0",
        "php-standard-library/phpstan-extension": "^1.0",
        "symfony/event-dispatcher": "^6.4.8",
        "symfony/clock": "^6.4.8",
        "wiremock-php/wiremock-php": "dev-cloud-support as 2.39.0",
        "php-soap/wsdl-reader": "^0.17.0",
        "roave/better-reflection": "^6.25.0",
        "symfony/finder": "^6.4.10"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "cweagans/composer-patches": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lstrojny/wiremock-php.git"
        }
    ]
}
